// add trailing units to an articulated vehicle
<tal:buyable_variants repeat="buyable_variant consist.cabbage_buyable_variants">
    switch (FEAT_TRAINS, SELF, ${consist.id}_articulated_cb_switch, extra_callback_info1) {
        <tal:trailing_units repeat="unit consist.units">
            ${repeat.unit.index}: return ${unit.id};
        </tal:trailing_units>
      return CB_RESULT_NO_MORE_ARTICULATED_PARTS;
    }
</tal:buyable_variants>
