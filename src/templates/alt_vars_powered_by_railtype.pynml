<tal:track_type_name repeat="track_type_name railtype_manager.railtype_labels_by_vehicle_track_type_name_cabbage">
    switch (FEAT_TRAINS, SELF, tile_powers_track_type_name_${track_type_name},
        [
            <!--! note that pipe | has to be escaped for chameleon as it has special meaning in TALES expressions -->
            ${" \|\| ".join(['tile_powers_railtype("' + railtype + '")' for railtype in railtype_manager.railtype_labels_by_vehicle_track_type_name_cabbage[track_type_name]])}
        ]) {
        return;
    }
</tal:track_type_name>
